{
  "name": "@elder-first/database",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "migrate": "node -r tsx/register src/migrate.ts",
    "migrate:up": "node -r tsx/register src/migrate.ts up",
    "migrate:down": "node -r tsx/register src/migrate.ts down",
    "seed": "node -r tsx/register src/seed.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/pg": "^8.10.9",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
